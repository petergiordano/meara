{
  "version": 2,
  "builds": [
    {
      "src": "deploy/vercel_frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "deploy/vercel_frontend/.next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/deploy/vercel_frontend/$1"
    }
  ]
}
